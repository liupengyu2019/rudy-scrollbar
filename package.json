  "version": "1.0.10",
  "author": "rudy <rudy_zhou@sina.cn>",
  "license": "MIT",
  "main": "dist/rudy-scrollbar.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rudyLittleLove/rudy-scrollbar"
  },
  "scripts": {
    "start": "npm run build & npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "rudy-scrollbar": "^1.0.10",
    "vue": "^2.6.11",
    "vue-router": "^2.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
